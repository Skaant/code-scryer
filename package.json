{
  "name": "code-scryer",
  "version": "0.1.0",
  "description": "Come aboard the Code Scryer to explore, visualize and interact with codebases !",
  "main": "README.md",
  "scripts": {
    "dev:server": "npx nodemon server/server.ts --watch server --watch _motifs",
    "ds": "npm run dev:server",
    "dev:front": "npx webpack -c front/webpack.config.js --watch",
    "df": "npm run dev:front",
    "test": "npx jest",
    "prod:server": "npx tsc-bundle server/tsconfig.json",
    "prod:front": "npx webpack -c front/webpack.config.js --mode=production",
    "start": "node server/server.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "babel-cli": "^6.26.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Skaant/code-scryer.git"
  },
  "keywords": [
    "developer",
    "experience"
  ],
  "author": "rimarok.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Skaant/code-scryer/issues"
  },
  "homepage": "https://github.com/Skaant/code-scryer#readme"
}
